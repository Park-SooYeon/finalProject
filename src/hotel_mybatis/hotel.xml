<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ht">

<!-- 호텔 탑메뉴 진입시 예약가능한 호텔을 평점순으로 보여줌니당 -->
<select id='mainNo'  resultType="bean.PlaceVo">
select 
		PLACE_SERIAL,PARTNER_SERIAL, PLACE_CODE, PLACE_NAME, 
		PLACE_LOCATION, LATITUDE, LONGITUDE, GRADE,  STATE,
	    LOCAL_CODE 
from place 
where place_code = 4 and local_code=1




</select>


